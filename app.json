{
  "expo": {
    "name": "WeatherApp Dev",
    "slug": "WeatherApp-dev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/iconDev.png",
    "scheme": "weatherappmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farcodefr.weatherapp.dev"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.farcodefr.weatherapp.dev"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo/iconDev.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "SFProRegular": "./assets/fonts/SFPRODISPLAYREGULAR.ttf",
          "SFProSemiboldItalic": "./assets/fonts/SFPRODISPLAYSEMIBOLDITALIC.ttf"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
